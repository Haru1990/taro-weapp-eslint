{
  "name": "weappTemplate",
  "version": "1.0.0",
  "private": true,
  "description": "A weapp template by taro",
  "templateInfo": {
    "name": "redux",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "format": "eslint --fix 'src/**/*.{js,ts,tsx}'"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/async-await": "1.3.5",
    "@tarojs/components": "1.3.5",
    "@tarojs/redux": "1.3.5",
    "@tarojs/redux-h5": "1.3.5",
    "@tarojs/router": "1.3.5",
    "@tarojs/taro": "1.3.5",
    "@tarojs/taro-alipay": "1.3.5",
    "@tarojs/taro-h5": "1.3.5",
    "@tarojs/taro-swan": "1.3.5",
    "@tarojs/taro-tt": "1.3.5",
    "@tarojs/taro-weapp": "1.3.5",
    "nerv-devtools": "^1.4.0",
    "nervjs": "^1.4.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-ui": "^2.2.2"
  },
  "devDependencies": {
    "@tarojs/plugin-babel": "1.3.5",
    "@tarojs/plugin-csso": "1.3.5",
    "@tarojs/plugin-less": "1.3.5",
    "@tarojs/plugin-sass": "^1.3.19",
    "@tarojs/plugin-uglifyjs": "1.3.5",
    "@tarojs/webpack-runner": "1.3.5",
    "@types/react": "^16.4.8",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-taro": "1.3.5",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-taro": "1.3.5",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "1.3.5",
    "stylelint-taro-rn": "1.3.5",
    "tslint": "^5.20.0",
    "tslint-sf-react": "^1.0.3",
    "typescript": "^3.0.1"
  }
}
